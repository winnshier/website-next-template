# 测试环境配置

# 环境标识
NODE_ENV=production
NEXT_PUBLIC_ENV=staging

# 站点配置
NEXT_PUBLIC_SITE_URL=https://staging.example.com

# CDN配置
NEXT_PUBLIC_CDN_URL=https://cdn-staging.example.com
IMAGE_CDN_TOKEN=staging_cdn_token_here

# API配置
NEXT_PUBLIC_API_URL=https://api-staging.example.com
API_SECRET_KEY=staging_api_secret_key_here

# 调试选项
NEXT_PUBLIC_SHOW_DEBUG_INFO=true
NEXT_PUBLIC_DEBUG=false

# 可选：数据库配置
# DATABASE_URL=postgresql://user:password@db-staging.example.com:5432/webtem_staging

# 可选：第三方服务
# GOOGLE_ANALYTICS_ID=G-STAGING-XXXXXXXXXX
# SENTRY_DSN=https://xxx@sentry.io/staging
